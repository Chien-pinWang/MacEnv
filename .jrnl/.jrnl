
alias +new="jrnl"
alias +last="jrnl -1"
alias +last5="jrnl -5"
alias +today="jrnl -on today"
alias +yesterday="jrnl -on yesterday"
alias +star="jrnl -starred"
alias +all="jrnl -from 2017-03-03 --short"

function jrnlReview() {
    while read line
    do
        echo -e "$line"
    done < ~/.env/.jrnl/jrnlReview.txt
}
function jrnlSearch () {
    jrnl -from 2017-01-01 | grep "$1"
}
