
alias jNew="jrnl"
alias jLast="jrnl -1"
alias jLast5="jrnl -5"
alias jToday="jrnl -on today"
alias jYesterday="jrnl -on yesterday"
alias jStar="jrnl -starred"
alias jAll="jrnl -from 2017-03-03 --short"

function j?() {
    while read line
    do
        echo -e "$line"
    done < ~/.env/.jrnl/jHint.txt
}
function jSearch () {
    jrnl -from 2017-01-01 | grep "$1"
}
